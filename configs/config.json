{
  "tcp": {
    "host": "0.0.0.0",
    "port": 8999,
    "maxConn": 10000,
    "workerPoolSize": 10,
    "maxWorkerTaskLen": 1024,
    "maxMsgChanLen": 1024
  },
  "http": {
    "host": "0.0.0.0",
    "port": 8080,
    "readTimeout": 10,
    "writeTimeout": 10,
    "maxHeaderBytes": 1048576
  },
  "log": {
    "level": "info",
    "path": "./logs",
    "maxSize": 100,
    "maxBackups": 5,
    "maxAge": 30,
    "compress": true
  }
}
