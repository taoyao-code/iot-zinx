{
  "Name": "Charging Gateway TCP Server",
  "Host": "0.0.0.0",
  "TCPPort": 7054,
  "MaxConn": 3000,
  "WorkerPoolSize": 10,
  "LogDir": "./logs",
  "LogFile": "zinx.log",
  "LogSaveDays": 15,
  "LogCons": true,
  "LogIsolationLevel": 0,
  "Mode": "tcp",
  "TCPWriteTimeout": "90s",
  "TCPReadTimeout": "300s",
  "SendBufferSize": 262144,
  "ReceiveBufferSize": 131072,
  "KeepAlive": true,
  "KeepAlivePeriod": "15s",
  "TCPNoDelay": true,
  "SendQueueSize": 1024,
  "ReadQueueSize": 1024,
  "WriteChannelBuffer": 512,
  "ReadChannelBuffer": 512,
  "MaxWorkerTaskLen": 1024,
  "MaxPacketSize": 2048
}
